{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
          "src": "/:id",
          "dest": "/index.js",
          "methods": ["GET"]
        },
        {
          "src": "/create-url",
          "dest": "/index.js",
          "methods": ["POST"]
        }
      ]
    
}